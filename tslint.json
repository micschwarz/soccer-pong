{
    "extends": "tslint-config-airbnb",
    "env": {
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "ter-indent": [true, 4],
        "no-console": [false],
        "arrow-parens": true,
        "linebreak-style": [true, "CRLF"],
        "import-name": [false]
    }
}